<Window x:Class="Client.UploadCompleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="UploadCompleteWindow" Height="126.473" Width="251.947" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid IsEnabled="True">
        <Label Content="Upload complete! Your download code is: " HorizontalAlignment="Left" Margin="9,9,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtDownloadCode" IsReadOnly="True" HorizontalAlignment="Left" Height="23" Margin="9,40,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" />
        <Button x:Name="btnCopy" Content="Copy to clipboard" HorizontalAlignment="Left" Margin="134,40,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="-0.059,0.397" Height="23" Click="btnCopy_Click"/>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Left" Margin="159,68,0,0" VerticalAlignment="Top" Width="75" Click="btnClose_Click"/>

    </Grid>
</Window>
